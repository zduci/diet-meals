= render 'autocomplete_js'

= form_for(@meal) do |f|
  %p
    = f.label :name
    = f.text_field :name

  %p
    = f.label :instructions
    = f.text_field :instructions

  %p
    = f.label :duration
    = f.text_field :duration

  = f.fields_for :ingredients do |builder|
    = render "ingredient_fields", :f => builder

  = f.submit 'Add Meal'
